<!DOCTYPE html>
<html lang="en">
  <%- include('head'); -%>

  <body>
    <%- include('nav'); -%>
    <main id="main" style="margin-top: 100px">
      <!-- ======= Breadcrumbs ======= -->
      <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">
          <div class="d-flex justify-content-between align-items-center">
            <h2><%= title %></h2>
            <ol>
              <li><a href="/">Home</a></li>
              <li><%= title %></li>
            </ol>
          </div>
        </div>
      </section>
      <!-- End Breadcrumbs -->

      <!-- ======= Portfolio Details Section ======= -->
      <section id="portfolio-details" class="portfolio-details">
        <div class="container" data-aos="fade-up">
          <div class="portfolio-details-container">
            <div class="owl-carousel portfolio-details-carousel">
              <% service.images.forEach(element => { %> <img src=<%=
              `/assets/img/portfolio/serviceImg/${element}.png` %>
              class="img-fluid" alt=<%= `${title}`%> /> <% }) %>
            </div>

            <div class="portfolio-info">
              <h3>Service : <%= title %></h3>
              <ul class="subServices">
                <% service.subServices.forEach(function(srv){ %>
                <li><%= srv %></li>
                <% }); %>
              </ul>
            </div>
          </div>

          <div class="portfolio-description">
            <% service.content.forEach(cont => { %> <% if (cont.type === 'para')
            { %>
            <p><%= cont.data %></p>
            <% } else if(cont.type === 'list') { %>
            <ul>
              <% cont.data.forEach(function(item){ %>
              <li><%= item %></li>
              <% }); %>
            </ul>
            <% } %> %> <% }) %>
          </div>
        </div>
      </section>
      <!-- End Portfolio Details Section -->
    </main>
    <%- include('footer'); -%>
    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>
    <div id="preloader"></div>

    <!-- Vendor JS Files -->

    <script src="/assets/vendor/jquery/jquery.min.js"></script>
    <script src="/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
    <!-- <script async src="/assets/vendor/php-email-form/validate.js"></script> -->
    <script src="/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="/assets/vendor/counterup/counterup.min.js"></script>
    <script src="/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="/assets/vendor/venobox/venobox.min.js"></script>
    <script src="/assets/vendor/aos/aos.js"></script>

    <!-- Template Main JS File -->
    <script src="/assets/js/main.js"></script>
  </body>
</html>
